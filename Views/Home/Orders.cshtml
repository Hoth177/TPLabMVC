@{
    ViewData["Title"] = "Список заказов";
}

<h2>
    Все заказы
</h2>
<table>
    <tr>
        <td>Имя покупателя</td>
        <td>Адрес</td>
        <td>Контактный телефон</td>
        <td>ИД продукта</td>
        <td>Стоимость</td>
        <td></td>
    </tr>
    @foreach (var order in Model)
    {
        <tr>
            <td>@order.User</td>
            <td>@order.Address</td>
            <td>@order.ContactPhone</td>
            <td>@order.ProductId</td>
            <td>@order.Cost</td>
        </tr>
    }
</table>
